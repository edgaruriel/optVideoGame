<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.testandroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LifeCycleTest"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".AndroidBasicStarter"
            android:label="@string/title_activity_touch" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AssetsTest"
            android:label="@string/title_activity_assets_test" >
        </activity>
        <activity
            android:name=".ExternalStorageTest"
            android:label="@string/title_activity_external_storage_test" >
        </activity>
        <activity
            android:name=".AccelerometerTest"
            android:label="Test Acelerometro"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".MultiTouchTest"
            android:label="@string/title_activity_multi_touch_test" >
        </activity>
        <activity
            android:name=".AccelerometerTest2"
            android:label="@string/title_activity_accelerometer_test2" >
        </activity>
        <activity
            android:name=".RenderViewTest"
            android:label="@string/title_activity_render_view_test" >
        </activity>
        <activity
            android:name=".ShapeTest"
            android:label="@string/title_activity_shape_test" >
        </activity>
    </application>

</manifest>
